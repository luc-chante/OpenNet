{
  "Logging": {
    "IncludeScopes": false,
    "Debug": {
      "LogLevel": {
        "Default": "Warning"
      }
    },
    "Console": {
      "LogLevel": {
        "Default": "Warning"
      }
    },
    "File": {
      "LogLevel": {
        "Default": "Warning"
      },
      "Path": "~/service.log",
      "Prefix": "%date [%lvl] %name - ",
      "Behaviour": "Backup",
      "BackupMode": "Startup, Age, Size",
      "MaxSize": "1M",
      "MaxAge": "1.00:00:00"
    }
  }
}
